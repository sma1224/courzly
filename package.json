{
  "name": "courzly",
  "version": "1.0.0",
  "description": "Dynamic Agent Platform for Course Creation",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run frontend:dev\" \"npm run api:dev\"",
    "frontend:dev": "cd frontend && npm start",
    "api:dev": "cd backend/api && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "build": "cd frontend && npm run build",
    "test": "npm run test:frontend && npm run test:api",
    "test:frontend": "cd frontend && npm test",
    "test:api": "cd backend/api && python -m pytest",
    "docker:dev": "docker-compose up -d",
    "docker:prod": "docker-compose -f docker/production/docker-compose.yml up -d",
    "deploy": "./scripts/deploy/production.sh",
    "setup:ssl": "./scripts/setup/ssl-setup.sh"
  },
  "keywords": [
    "ai",
    "agents",
    "course-creation",
    "flowise",
    "automation",
    "education"
  ],
  "author": "Courzly Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}